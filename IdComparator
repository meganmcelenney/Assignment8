import java.util.Comparator;

public class IdComparator implements Comparator<Food>
{
	public int compare(Food one, Food two)
	{
		if(one.getId() < two.getId())
			return -2;
		else if(one.getId() > two.getId())
			return 2;
		else
			return 0;
	}

}
